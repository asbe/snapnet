    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Python: run setup",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": [
                "install",
                "--home=\".\""
            ],
            "cwd": "${fileDirname}"
        },
        {
            "name": "Python: gen images",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": [
                "--config",
                "config.json"
            ],
            "cwd": "${fileDirname}"
        }
    ]
}